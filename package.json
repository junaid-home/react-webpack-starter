{
  "name": "react-webpack-starter",
  "description": "️‍simple boilerplate for creating React applications.",
  "author": "Junaid Javed <twitter@junaid_home> (https://github.com/junaid-home)",
  "license": "MIT",
  "keywords": [
    "ES6",
    "react",
    "babel",
    "webpack5"
  ],
  "version": "1.0.1",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --config ./.webpack/webpack.config.js",
    "build": "webpack --env production --config ./.webpack/webpack.config.js",
    "lint": "eslint --ext .js,.jsx .",
    "prettier": "prettier --ignore-path ./.gitignore .",
    "format": "npm run prettier -- --write",
    "check:format": "npm run prettier -- --list-different"
  },
  "dependencies": {
    "react": "*",
    "react-dom": "*",
    "react-router-dom": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-webpack-plugin": "^2.4.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0-beta.4",
    "mini-css-extract-plugin": "^1.3.3",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3",
    "webpack-pwa-manifest": "^4.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/junaid-home/react-webpack-starter"
  },
  "bugs": {
    "url": "https://github.com/junaid-home/react-webpack-starter/issues"
  },
  "homepage": "https://github.com/junaid-home/react-webpack-starter#readme"
}
